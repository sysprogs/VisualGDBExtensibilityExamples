<UserControl x:Class="ImageVisualWatch.ImageWatchViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ImageVisualWatch"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <ResourceDictionary>
            <x:Array Type="UIElement" x:Key="ToolbarButtons">
                <Button Click="ZoomOut_Click" ToolTip="Reset zoom level">
                    <Image Width="16" Height="16" Source="Icons\ZoomOut_16x.png"/>
                </Button>
            </x:Array>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
            
    </Grid>
</UserControl>
